struct ListNode # Definition for singly-linked list.
 {
      int val;
      struct ListNode *next;
 };
struct ListNode* reverseList(struct ListNode* head) # reverse list function
{
   struct ListNode* prevnode=NULL,*currentnode=head,*nextnode=head;
   while(nextnode!=NULL)
     {
           nextnode=nextnode->next;
           currentnode->next=prevnode;
           prevnode=currentnode;
           currentnode=nextnode;
     }
   head=prevnode;
   return head;
}

  
